mtgoc:Artifact a sh:NodeShape ;
    sh:closed true ;
    sh:description "A permanent representing a magical item, animated construct, or other objects and devices." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class mtgoa:Ability ;
            sh:description "An ability associated with a card." ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path mtgoc:ability ],
        [ sh:description "An ability associated with a card." ;
            sh:in ( "common" "uncommon" "rare" "mythic" ) ;
            sh:maxCount 1 ;
            sh:order 11 ;
            sh:path mtgoc:rarity ],
        [ sh:description "A pool of cards released together and designed for the same play environment." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path mtgoc:card_set ],
