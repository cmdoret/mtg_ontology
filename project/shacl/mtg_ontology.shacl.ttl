schema:Thing a sh:NodeShape ;
    sh:closed true ;
    sh:description "A generic grouping for any identifiable entity" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path schema:name ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path schema:identifier ],
        [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path schema:description ] ;
    sh:targetClass schema:Thing .

mtgo:ActivatedAbility a sh:NodeShape ;
    sh:closed true ;
    sh:description "A card ability which can be activated for a cost." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path schema:name ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path schema:identifier ],
        [ sh:description "The text describing what an ability does." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path mtgo:rules_text ],
        [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path schema:description ],
        [ sh:description "An additional cost to play a card or ability." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path mtgo:additional_cost ] ;
    sh:targetClass mtgo:ActivatedAbility .

mtgo:Artifact a sh:NodeShape ;
    sh:closed true ;
    sh:description "A permanent representing a magical item, animated construct, or other objects and devices." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 12 ;
            sh:path schema:identifier ],
        [ sh:description "The subtype(s) of a card, such as Equipment, Cat or Warrior. Appears on the right of the type line." ;
            sh:order 10 ;
            sh:path mtgo:card_subtype ],
        [ sh:description "The artist who drew a card's illustration." ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path mtgo:artist ],
        [ sh:description "An additional cost to play a card or ability." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path mtgo:additional_cost ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 13 ;
            sh:path schema:name ],
        [ sh:description "The color of a card." ;
            sh:in ( wiki:Q23445 wiki:Q1088 wiki:Q3133 wiki:Q3142 wiki:Q23444 wiki:Q1396399 ) ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path mtgo:color ],
        [ sh:class mtgo:ManaCost ;
            sh:description "A mana cost for a card or ability." ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path mtgo:mana_cost ],
        [ sh:description "The total mana cost for a card or ability, summed across all colors." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path mtgo:converted_mana_cost ],
        [ sh:description "A pool of cards released together and designed for the same play environment." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path mtgo:card_set ],
        [ sh:description "The lore text at the bottom of a card." ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path mtgo:flavor_text ],
        [ sh:description "The supertype(s) of a card, such as Basic, Legendary or Snow. Appears on the left of the type line, before the type." ;
            sh:order 11 ;
            sh:path mtgo:card_supertype ],
        [ sh:description "The text line below a card's illustration containing the card's type, subtype and supertype (if applicable)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 9 ;
            sh:path mtgo:card_type ],
        [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 14 ;
            sh:path schema:description ],
        [ sh:description "The text line below a card's illustration containing the card's type, subtype and supertype (if applicable)." ;
            sh:maxCount 1 ;
