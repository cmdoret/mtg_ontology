mtgoa:AbilityCollection a sh:NodeShape ;
    sh:closed true ;
    sh:description "A collection of abilities and associated concepts." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class mtgoa:ActionSpecification ;
            sh:description "Action specifications in the collection." ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path mtgoa:action_specifications ],
        [ sh:class mtgoa:ActivatedAbility ;
            sh:description "Activated abilities in the collection." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path mtgoa:activated_abilities ],
        [ sh:class mtgoa:TimeSpecification ;
            sh:description "Time specifications in the collection." ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path mtgoa:time_specifications ],
        [ sh:class mtgoc:ManaCost ;
            sh:description "Mana costs in the collection." ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path mtgoa:mana_costs ],
        [ sh:class mtgoa:Condition ;
            sh:description "Conditions in the collection." ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path mtgoa:conditions ],
        [ sh:class mtgoa:ValueSpecification ;
            sh:description "Values in the collection." ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path mtgoa:value_specifications ] ;
    sh:targetClass mtgoa:AbilityCollection .

mtgoa:KeywordAbility a sh:NodeShape ;
    sh:closed true ;
    sh:description "A passive ability represented by word that substitutes for a piece of rules text, such as deathtouch or flying." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path schema:identifier ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path schema:name ],
        [ sh:description "The text describing what an ability does." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path mtgoa:rules_text ],
        [ sh:class mtgoa:Condition ;
            sh:description "The effect of an ability." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path mtgoa:effect ],
        [ sh:description "The numeric part of a quantitative value." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path mtgoc:value ] ;
    sh:targetClass mtgoa:KeywordAbility .

mtgoa:Specification a sh:NodeShape ;
    sh:closed true ;
    sh:description "A specification for a thing." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class schema:Thing ;
            sh:description "A disjunction of two objects (a or b)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path mtgo:union ],
        [ sh:class schema:Thing ;
            sh:description "A conjunction of two objects (a and b)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path mtgo:intersection ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path schema:identifier ],
        [ sh:description "A constraint on a time, quantity or action." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path mtgoa:constraint ] ;
    sh:targetClass mtgoa:Specification .

mtgoa:StaticAbility a sh:NodeShape ;
    sh:closed true ;
    sh:description "A card ability which remains active until endgame once put into effect." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path schema:identifier ],
        [ sh:description "The text describing what an ability does." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path mtgoa:rules_text ],
        [ sh:class mtgoa:Condition ;
            sh:description "The effect of an ability." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path mtgoa:effect ] ;
    sh:targetClass mtgoa:StaticAbility .

mtgoa:TriggeredAbility a sh:NodeShape ;
    sh:closed true ;
