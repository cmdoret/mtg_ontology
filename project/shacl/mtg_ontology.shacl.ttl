mtgo:AbilityCollection a sh:NodeShape ;
    sh:closed true ;
    sh:description "A collection of abilities and associated concepts." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class mtgo:ValueSpecification ;
            sh:description "Values in the collection." ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path mtgo:value_specifications ],
        [ sh:class mtgo:ActionSpecification ;
            sh:description "Action specifications in the collection." ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path mtgo:action_specifications ],
        [ sh:class mtgo:Condition ;
            sh:description "Conditions in the collection." ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path mtgo:conditions ],
        [ sh:class mtgo:ManaCost ;
            sh:description "Mana costs in the collection." ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path mtgo:mana_costs ],
        [ sh:class mtgo:TimeSpecification ;
            sh:description "Time specifications in the collection." ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path mtgo:time_specifications ],
        [ sh:class mtgo:ActivatedAbility ;
            sh:description "Activated abilities in the collection." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path mtgo:activated_abilities ] ;
    sh:targetClass mtgo:AbilityCollection .

mtgo:AnyArtifact a sh:NodeShape ;
    sh:closed false ;
    sh:description "A permanent representing a magical item, animated construct, or other objects and devices." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path schema:description ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path schema:name ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path schema:identifier ] ;
    sh:targetClass mtgo:AnyArtifact .

mtgo:AnyCreature a sh:NodeShape ;
    sh:closed false ;
    sh:description "A creature that can be summoned to the battlefield." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The amount of damage dealt in combat. This is the first number in the lower right corner of a creature card." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path mtgo:power ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path schema:identifier ],
        [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path schema:description ],
        [ sh:description "The amount of damage needed to destroy a creature. This is the second number in the lower right corner of a creature card." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path mtgo:toughness ],
        [ sh:description "Whether a creature's power is variable. Denoted as * on cards." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path mtgo:variable_power ],
        [ sh:description "Whether a creature's toughness is variable. Denoted as * on cards." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path mtgo:variable_toughness ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 5 ;
            sh:path schema:name ] ;
    sh:targetClass mtgo:AnyCreature .

mtgo:AnyEnchantment a sh:NodeShape ;
    sh:closed false ;
    sh:description "A permanent which applies persistent magical effects." ;
    sh:ignoredProperties ( rdf:type ) ;
