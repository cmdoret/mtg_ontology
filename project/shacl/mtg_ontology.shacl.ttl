schema:Thing a sh:NodeShape ;
    sh:closed true ;
    sh:description "A generic grouping for any identifiable entity" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path schema:description ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path schema:name ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path schema:identifier ] ;
    sh:targetClass schema:Thing .

mtgoa:Ability a sh:NodeShape ;
    sh:closed true ;
    sh:description "A card ability, activated, triggered or static." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path schema:description ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path schema:name ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path schema:identifier ],
        [ sh:description "The text describing what an ability does." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path mtgoa:rules_text ] ;
    sh:targetClass mtgoa:Ability .

mtgoa:ActivatedAbility a sh:NodeShape ;
    sh:closed true ;
    sh:description "A card ability which can be activated for a cost." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The text describing what an ability does." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path mtgoa:rules_text ],
        [ sh:description "An additional cost to play a card or ability." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path mtgoc:additional_cost ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path schema:name ],
        [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path schema:description ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path schema:identifier ] ;
    sh:targetClass mtgoa:ActivatedAbility .

mtgoa:Condition a sh:NodeShape ;
    sh:closed true ;
    sh:description "A condition expressed as a constraint on a property." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class <rdf:predicate> ;
            sh:description "A property of an entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path mtgoa:property ],
        [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path schema:name ],
        [ sh:description "A constraint on the value of a property." ;
            sh:in ( "equal" "greater_than" "less_than" "greater_than_or_equal" "less_than_or_equal" "not_equal" "between" "not_between" "is" "is_not" "is_one_of" "is_not_one_of" ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path mtgoa:value_constraint ],
        [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path schema:description ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path schema:identifier ],
        [ sh:description "The entity targeted by an action or constraint." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path mtgoa:target ] ;
    sh:targetClass mtgoa:Condition .

mtgoa:KeywordAbility a sh:NodeShape ;
    sh:closed true ;
    sh:description "A passive ability represented by word that substitutes for a piece of rules text, such as deathtouch or flying." ;
