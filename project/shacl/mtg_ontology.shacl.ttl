mtgo:Artifact a sh:NodeShape ;
    sh:closed true ;
    sh:description "A card that represents an artifact that can be cast and put onto the battlefield." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "A human-readable name for a thing" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 14 ;
            sh:path schema:name ],
        [ sh:description "An ability associated with a card." ;
            sh:in ( mtg:common mtg:uncommon mtg:rare mtg:mythic_rare ) ;
            sh:maxCount 1 ;
            sh:order 11 ;
            sh:path mtgo:rarity ],
        [ sh:description "The lore text at the bottom of a card." ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path mtgo:flavor_text ],
        [ sh:class mtgo:ManaCost ;
            sh:description "A mana cost for a card or ability." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path mtgo:mana_cost ],
        [ sh:description "The total mana cost for a card or ability, summed across all colors." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path mtgo:converted_mana_cost ],
        [ sh:class mtgo:Ability ;
            sh:description "An ability associated with a card." ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path mtgo:ability ],
        [ sh:description "A unique identifier for a thing" ;
            sh:maxCount 1 ;
            sh:order 13 ;
            sh:path schema:identifier ],
        [ sh:description "The text line below a card's illustration containing the card's type, subtype and supertype (if applicable)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 7 ;
            sh:path mtgo:type_line ],
        [ sh:description "A human-readable description for a thing" ;
            sh:maxCount 1 ;
            sh:order 15 ;
            sh:path schema:description ],
        [ sh:description "A pool of cards released together and designed for the same play environment." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path mtgo:card_set ],
        [ sh:description "The text line below a card's illustration containing the card's type, subtype and supertype (if applicable)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 8 ;
            sh:path mtgo:card_type ],
        [ sh:description "The supertype(s) of a card, such as Basic, Legendary or Snow. Appears on the left of the type line, before the type." ;
            sh:order 10 ;
            sh:path mtgo:card_supertype ],
        [ sh:description "The official wording for the card effects." ;
            sh:maxCount 1 ;
            sh:order 12 ;
            sh:path mtgo:oracle_text ],
