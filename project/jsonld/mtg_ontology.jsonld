{
  "name": "mtg-ontology",
  "description": "An ontology describing Magic: The Gathering.",
  "title": "mtg-ontology",
  "see_also": [
    "https://cmdoret.github.io/mtg-ontology"
  ],
  "id": "https://w3id.org/cmdoret/mtg-ontology",
  "imports": [
    "linkml:types"
  ],
  "license": "GNU GPL v3.0",
  "prefixes": [
    {
      "prefix_prefix": "mtgo",
      "prefix_reference": "https://w3id.org/cmdoret/mtg-ontology/"
    },
    {
      "prefix_prefix": "linkml",
      "prefix_reference": "https://w3id.org/linkml/"
    },
    {
      "prefix_prefix": "schema",
      "prefix_reference": "http://schema.org/"
    },
    {
      "prefix_prefix": "wiki",
      "prefix_reference": "http://en.wikipedia.org/wiki/"
    },
    {
      "prefix_prefix": "example",
